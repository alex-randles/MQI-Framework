@base <http://purl.org/eis/vocab/daq#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dc: <../../dc/elements/1.1/> .
@prefix dct: <../../dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qb: <../../linked-data/cube#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <../../vocab/vann/> .

<>
    dc:description "The Dataset Quality Vocabulary (daQ) is a lightweight,Observation extensible core vocabulary for attaching the result of quality benchmarking of a linked open dataset (usually an expensive process) to that dataset.  daQ is designed to be extended by custom quality metrics.  Use cases include filtering and ranking datasets by quality."@en ;
    dct:contributor [
        rdfs:seeAlso <../../net/clange> ;
        foaf:homepage <http://langec.wordpress.com/about/> ;
        foaf:name "Christoph Lange"
    ] ;
    dct:creator [
        foaf:mbox <mailto:debattij@scss.tcd.ie> ;
        foaf:name "Jeremy Debattista"
    ] ;
    dct:language "en"^^<http://www.w3.org/2001/XMLSchema#language> ;
    dct:modified "2017-08-03"^^<http://www.w3.org/2001/XMLSchema#date> ;
    dct:title "Dataset Quality Vocabulary (daQ)"@en ;
    vann:preferredNamespacePrefix "daq" ;
    vann:preferredNamespaceUri "http://purl.org/eis/vocab/daq#" ;
    a <../../vocommons/voaf#Vocabulary>, owl:Ontology ;
    foaf:homepage <> .

<http://purl.org/eis/vocab/daq#Category>
    a rdfs:Class, owl:Class ;
    rdfs:comment "The highest level of quality metric is a category. A category groups a number of dimensions relevant to each other which aims at measuring the quality of a dataset from different aspects. Categories are provided as subclasses of this abstract class, which is not intended for direct usage."@en ;
    rdfs:label "Category"@en ;
    owl:equivalentClass <http://www.w3.org/ns/dqv#Category> .

<http://purl.org/eis/vocab/daq#Dimension>
    a rdfs:Class, owl:Class ;
    rdfs:comment "Each dimension is part of a larger group called category (See daq:Category). Each dimension has a number of metrics which are associated to it. A dimension is linked with a category using the daq:hasDimension property. Dimensions are provided as subclasses of this abstract class, which is not intended for direct usage."@en ;
    rdfs:label "Dimension"@en ;
    owl:equivalentClass <http://www.w3.org/ns/dqv#Dimension> .

<http://purl.org/eis/vocab/daq#Metric>
    a rdfs:Class, owl:Class ;
    rdfs:comment "The smallest unit of measuring a quality dimension is a metric. A metric belongs to exactly one dimension. Each metric has one or more observations (daq:hasObservation), which records data quality assessment value following a computation. Metrics are provided as subclasses of this abstract class, which is not intended for direct usage."@en ;
    rdfs:label "Metric"@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:minCardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <http://purl.org/eis/vocab/daq#hasObservation>
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <http://purl.org/eis/vocab/daq#expectedDataType>
    ] ;
    owl:equivalentClass <http://www.w3.org/ns/dqv#Metric> .

<http://purl.org/eis/vocab/daq#Observation>
    a rdfs:Class, owl:Class ;
    rdfs:comment "A quality observation represents the statistical and provenance information of the attached metric's assessment activity."@en ;
    rdfs:label "Quality Observation"@en ;
    rdfs:subClassOf qb:Observation, <http://www.w3.org/ns/prov#Entity>, [
        a owl:Restriction ;
        owl:minCardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <http://purl.org/eis/vocab/daq#metric>
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <http://purl.org/eis/vocab/daq#value>
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <../../linked-data/sdmx/2009/dimension#timePeriod>
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <http://purl.org/eis/vocab/daq#computedOn>
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^<http://www.w3.org/2001/XMLSchema#nonNegativeInteger> ;
        owl:onProperty <http://purl.org/eis/vocab/daq#isEstimate>
    ] ;
    owl:equivalentClass <http://www.w3.org/ns/dqv#QualityMeasurement> .

<http://purl.org/eis/vocab/daq#QualityGraph>
    a rdfs:Class, owl:Class ;
    rdfs:comment "Defines a quality graph which will contain all metadata about quality metrics on the dataset."@en ;
    rdfs:label "Quality Graph Statistics"@en ;
    rdfs:subClassOf qb:DataSet, <http://www.w3.org/2004/03/trix/rdfg-1/Graph>, [
        a owl:Restriction ;
        owl:hasValue <http://purl.org/eis/vocab/daq#dsd> ;
        owl:onProperty qb:structure
    ] ;
    owl:equivalentClass <http://www.w3.org/ns/dqv#QualityMeasurementDataset> .

<http://purl.org/eis/vocab/daq#computedBy>
    a owl:DeprecatedProperty ;
    rdfs:comment "**Deprecated Property**. The computedBy property defines the Agent that computed a metric on a dataset."@en ;
    rdfs:domain qb:Observation ;
    rdfs:label "computed by"@en .

<http://purl.org/eis/vocab/daq#computedOn>
    a qb:DimensionProperty, rdf:Property, owl:ObjectProperty ;
    rdfs:comment "Quality metrics can be (in principle) calculated on various forms of data (such as datasets, graphs, set of triples etc...). This vocabulary allow the owner/user of such RDF data to calculate metrics on multiple (and different) resources."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Observation> ;
    rdfs:label "computed on"@en ;
    rdfs:range rdfs:Resource ;
    owl:equivalentProperty <http://www.w3.org/ns/dqv#computedOn> .

<http://purl.org/eis/vocab/daq#dsd>
    qb:component [
        qb:dimension <http://purl.org/eis/vocab/daq#metric> ;
        qb:order 1
    ], [
        qb:dimension <http://purl.org/eis/vocab/daq#computedOn> ;
        qb:order 2
    ], [
        qb:dimension <../../linked-data/sdmx/2009/dimension#timePeriod> ;
        qb:order 3
    ], [
        qb:measure <http://purl.org/eis/vocab/daq#value>
    ] ;
    a qb:DataStructureDefinition .

<http://purl.org/eis/vocab/daq#expectedDataType>
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Each metric should have an expect data type for it's observed value (e.g. xsd:boolean, xsd:double etc...)"@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Metric> ;
    rdfs:label "expected data type"@en ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#anySimpleType> ;
    owl:equivalentProperty <http://www.w3.org/ns/dqv#expectedDataType> .

<http://purl.org/eis/vocab/daq#hasDimension>
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "The category concept classifies dimensions related to the measurement of quality for a specific criteria. This is an abstract property and should not be used directly. Specific sub-properties should be inherited for different dimensions."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Category> ;
    rdfs:label "hasDimension"@en ;
    rdfs:range <http://purl.org/eis/vocab/daq#Dimension> .

<http://purl.org/eis/vocab/daq#hasMetric>
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "A dimension is an abstract concept which groups an number of more concrete metrics to measure quality of a dataset. This is an abstract property and should not be used directly. Specific sub-properties should be inherited for different metrics."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Dimension> ;
    rdfs:label "hasMetric"@en ;
    rdfs:range <http://purl.org/eis/vocab/daq#Metric> .

<http://purl.org/eis/vocab/daq#hasObservation>
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "Computed metrics can have 1 or more quality observations, where each computed resource has one observation."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Metric> ;
    rdfs:label "has observation"@en ;
    rdfs:range <http://purl.org/eis/vocab/daq#Observation> ;
    owl:inverseOf <http://purl.org/eis/vocab/daq#metric> .

<http://purl.org/eis/vocab/daq#isEstimate>
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "This property flags true if an assessed observation of a metric gives an estimate result instead of a more accurate one."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Observation> ;
    rdfs:label "is estimate"@en ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#boolean> .

<http://purl.org/eis/vocab/daq#metric>
    a qb:DimensionProperty, rdf:Property, owl:ObjectProperty ;
    rdfs:comment "Represents the metric being observed."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Observation> ;
    rdfs:label "metric"@en ;
    rdfs:range <http://purl.org/eis/vocab/daq#Metric> ;
    owl:equivalentProperty <http://www.w3.org/ns/dqv#isMeasurementOf> ;
    owl:inverseOf <http://purl.org/eis/vocab/daq#hasObservation> .

<http://purl.org/eis/vocab/daq#requires>
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "A metric might require a number of external resources (e.g. a gold standard) in order to be able to measure the quality. In order to cater for the most generic requirement, this property links a metric to the required resource (e.g. a URI to the gold standard dataset used)."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Metric> ;
    rdfs:label "requires"@en ;
    rdfs:range rdfs:Resource .

<http://purl.org/eis/vocab/daq#value>
    a qb:MeasureProperty, rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Each metric will have a value computed. In order to deal with the different return type of the metric computation, this property links a metric with a value object (e.g. boolean, double, Literal)."@en ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Observation> ;
    rdfs:label "value"@en ;
    owl:equivalentProperty <http://www.w3.org/ns/dqv#value> .

<../../linked-data/sdmx/2009/dimension#timePeriod>
    a qb:DimensionProperty ;
    rdfs:domain <http://purl.org/eis/vocab/daq#Observation> ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#dateTime> .

[]
    owl:equivalentProperty <http://www.w3.org/ns/dqv#hasQualityMeasurement> ;
    owl:inverseOf <http://purl.org/eis/vocab/daq#computedOn> .

[]
    owl:equivalentProperty <http://www.w3.org/ns/dqv#inCategory> ;
    owl:inverseOf <http://purl.org/eis/vocab/daq#hasDimension> .

[]
    owl:equivalentProperty <http://www.w3.org/ns/dqv#inDimension> ;
    owl:inverseOf <http://purl.org/eis/vocab/daq#hasMetric> .


###  http://w3id.org/FAIRVASC#ESKDDialysis
:ESKDDialysis rdf:type owl:DatatypeProperty ;
              rdfs:domain :ClinicalOutcomes ;
              rdfs:range xsd:boolean ;
              rdfs:comment "Dialysis indicating end stage kidney disease occured"@en ;
              rdfs:label "dialysis indicating end stage kidney disease"@en ,
                         "dialysis indicating end stage kidney disease" .


###  http://w3id.org/FAIRVASC#ESKDStandard
:ESKDStandard rdf:type owl:DatatypeProperty ;
              rdfs:domain :ClinicalOutcomes ;
              rdfs:range xsd:boolean ;
              rdfs:comment """The standard defintion for ESKD in FAIRVASC is defined as any of:
a) dialysis for >90 days;
b) sustained CKD 5 (eGFR < 15 ml/min/1.73m2) for >90 days; and/or
c) kidney transplantation"""@en ;
              rdfs:label "standard defintion of end stage kidney disease"@en ,
                         "standard defintion of end stage kidney disease" .



@prefix ns1: <https://w3id.org/> .
@prefix ns2: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns3: <http://purl.org/dc/terms/> .
@prefix ns4: <http://www.w3.org/2002/07/owl#> .
@prefix ns5: <http://purl.org/vocab/vann/> .
@prefix ns6: <http://purl.1122org/ontology/bibo/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:MQV {
    ns6:status a ns4:AnnotationProperty .

    ns3:abstract a ns4:AnnotationProperty .

    ns3:created a ns4:AnnotationProperty .

    ns3:title a ns4:AnnotationProperty .

    ns5:preferredNamespacePrefix a ns4:AnnotationProperty .

    ns5:preferredNamespaceUri a ns4:AnnotationProperty .

    ns2:example a ns4:AnnotationProperty .

    <https://alex-randles.github.io/MQV/#> a ns4:Ontology ;
        ns6:status "Draft" ;
        ns3:abstract "The Mapping Quality Vocabulary (MQV) provides a set of classes, properties, and restrictions that can be used to represent and interchange provenance and metadata information relating to the quality assessment, quality refinement and quality validation of mapping definitions used to generate Linked data datasets. It can also be specialized to create new classes and properties to model provenance and metadata information for domain specific mapping languages" ;
        ns3:created "1 June 2020" ;
        ns3:title "Mapping Quality Vocabulary" ;
        ns5:preferredNamespacePrefix "mqv" ;
        ns5:preferredNamespaceUri "https://alex-randles.github.io/MQV/#" .

    <https://alex-randles.github.io/MQV/#assessedMapping> a ns4:ObjectProperty ;
        rdfs:label "assessedMapping"@en ;
        rdfs:comment "The quality assessment of the mapping document created by the refinement process." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingDocument> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:assessedMapping :mappingDocument;
.

:mappingDocument
	a mqv:MappingDocument;
.""" .

    <https://alex-randles.github.io/MQV/#assessmentPerformedBy> a ns4:ObjectProperty ;
        rdfs:label "assessmentPerformedBy"@en ;
        rdfs:comment "The agent who performed the assessment of the mapping." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <http://www.w3.org/ns/prov#Agent> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:assessmentPerformedBy :AlexRandles;
.

:AlexRandles
	a prov:Agent;
.""" .

    <https://alex-randles.github.io/MQV/#createdBy> a ns4:ObjectProperty ;
        rdfs:label "createdBy"@en ;
        rdfs:comment "The agent or mapping quality refinement process who produced the mapping document." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingDocument> ;
        rdfs:range [ a ns4:Class ;
                ns4:unionOf ( <http://www.w3.org/ns/prov#Agent> <https://alex-randles.github.io/MQV/#MappingRefinement> ) ] ;
        ns2:example """@prefix :     <http://example.com/>.;	a mqv:MappingAssessment ;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingDocument
	a mqv:MappingDocument;
	mqv:createdBy :mappingEngineer;
.

:mappingEngineer
	a prov:Agent;
.""" .

    <https://alex-randles.github.io/MQV/#executes> a ns4:ObjectProperty ;
        rdfs:label "executes"@en ;
        rdfs:comment "The quality metric being executed by the mapping quality assessment process." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <http://www.w3.org/ns/dqv#Metric> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:executes :qualityMetric;
.

:qualityMetric
	a mqv:QualityMetric;
.""" .

    <https://alex-randles.github.io/MQV/#hasConfidenceScore> a ns4:DatatypeProperty ;
        rdfs:label "hasConfidenceScore"@en ;
        rdfs:comment "The confidence score of a mapping refinement." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingRefinement> ;
        rdfs:range xsd:decimal ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingRefinement
	a mqv:MappingRefinement;
	mqv:hasConfidenceScore "0.8"^^xsd:double;
.""" .

    <https://alex-randles.github.io/MQV/#hasLocation> a ns4:DatatypeProperty ;
        rdfs:label "hasLocation"@en ;
        rdfs:comment "The location of a violation within the mapping." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingViolation> ;
        rdfs:range xsd:string ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingViolation
	a mqv:MappingViolation;
	mqv:hasLocation "Predicate Object Map"^^xsd:string.
""" .

    <https://alex-randles.github.io/MQV/#hasMetricType> a ns4:ObjectProperty ;
        rdfs:label "hasMetricType"@en ;
        rdfs:comment "A type assigned to a mapping quality metric. For instance, vocabulary type, for a mapping quality metric, which relates to the vocabulary being used within the mapping." ;
        rdfs:domain <http://www.w3.org/ns/dqv#Metric> ;
        rdfs:range <https://alex-randles.github.io/MQV/#QualityMetricType> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:qualityMetric
	a dqv:Metric;
	mqv:hasMetricType :dataQualityMetric;
.

:dataQualityMetric
	a mqv:QualityMetricType;
.""" .

    <https://alex-randles.github.io/MQV/#hasQualityMeasurement> a ns4:ObjectProperty ;
        rdfs:label "hasQualityMeasurement"@en ;
        rdfs:comment "A measurement calculated  by a quality metric on a mapping document." ;
        rdfs:domain [ a ns4:Class ;
                ns4:unionOf ( <https://alex-randles.github.io/MQV/#MappingDocument> <https://alex-randles.github.io/MQV/#QualityRequirement> ) ] ;
        rdfs:range [ a ns4:Class ;
                ns4:unionOf ( <http://www.w3.org/ns/dqv#Category> <http://www.w3.org/ns/dqv#Dimension> <http://www.w3.org/ns/dqv#Metric> <http://www.w3.org/ns/dqv#QualityMeasurement> ) ] ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingDocument
	a mqv:MappingDocument;
	mqv:hasQualityMeasurement :qualityMeasurement;
.

:qualityMeasurement
	a dqv:QualityMeasurement;
.""" .

    <https://alex-randles.github.io/MQV/#hasQualityRequirement> a ns4:ObjectProperty ;
        rdfs:label "hasQualityRequirement"@en ;
        rdfs:comment "The quality requirement of a quality assessment process." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <https://alex-randles.github.io/MQV/#QualityRequirement> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:hasQualityRequirement :qualityRequirement;
.

:qualityRequirement
	a mqv:QualityRequirement;
.""" .

    <https://alex-randles.github.io/MQV/#hasRefinement> a ns4:ObjectProperty ;
        rdfs:label "hasRefinement"@en ;
        rdfs:comment "The refinement used for refining a quality violation detcted by a particular quality metric." ;
        rdfs:domain <http://www.w3.org/ns/dqv#Metric> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingRefinement> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:qualityMetric
	a dqv:Metric;
	mqv:hasRefinement :mappingRefinement;
.

:mappingRefinement
	a mqv:MappingRefinement;
.""" .

    <https://alex-randles.github.io/MQV/#hasRefinementQuery> a ns4:DatatypeProperty ;
        rdfs:label "hasRefinementQuery"@en ;
        rdfs:comment "A query executed during by a refinement." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingRefinement> ;
        rdfs:range xsd:string ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:refinement1 a mqv:MappingRefinement;
	mqv:hasRefinementQuery '''
		PREFIX rr: <http://www.w3.org/ns/r2rml#>
		PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
		PREFIX foaf: <http://xmlns.com/foaf/0.1/>
		DELETE{?om rr:datatype ?datatype}
		INSERT{?om rr:datatype xsd:string }
		WHERE
		{
		?sub rr:predicateObjectMap ?pom.
		?pom rr:predicate foaf:name.
		?pom rr:objectMap ?om.
		?om rr:datatype ?datatype.
		}'''^^xsd:string .
""" .

    <https://alex-randles.github.io/MQV/#hasValidationReport> a ns4:ObjectProperty ;
        rdfs:label "hasValidationReport"@en ;
        rdfs:comment "The mapping quality validation report generated by the quality assessment process." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingValidationReport> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:hasValidationReport :mappingValidationReport;
.

:mappingValidationReport
	a mqv:MappingValidationReport;
	mqv:hasViolation :mappingViolation;
.""" .

    <https://alex-randles.github.io/MQV/#hasValue> a ns4:DatatypeProperty ;
        rdfs:label "hasValue"@en ;
        rdfs:comment "The value associated with the violation." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingViolation> ;
        rdfs:range xsd:string ;
        rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingViolation
	a mqv:MappingViolation;
	mqv:hasValue "en-GB".
""" .

    <https://alex-randles.github.io/MQV/#hasViolation> a ns4:ObjectProperty ;
        rdfs:label "hasViolation"@en ;
        rdfs:comment "A mapping quality violation contained within a mapping validation report." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingValidationReport> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingViolation> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingValidationReport
	a mqv:MappingValidationReport;
	mqv:hasViolation :mappingViolation;
.

:mappingViolation
	a mqv:MappingViolation;
.""" .

    <https://alex-randles.github.io/MQV/#inTripleMap> a ns4:ObjectProperty ;
        rdfs:label "inTripleMap"@en ;
        rdfs:comment "The triple map which contains the violation." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingViolation> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingViolation
	a mqv:MappingViolation;
	mqv:inTripleMap <#TripleMap1>;
.
""" .

    <https://alex-randles.github.io/MQV/#isDescribedBy> a ns4:ObjectProperty ;
        rdfs:label "isDescribedBy"@en ;
        rdfs:comment "The quality metric which detected a quality violation." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingViolation> ;
        rdfs:range <http://www.w3.org/ns/dqv#Metric> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingViolation
	a mqv:MappingViolation;
	mqv:isDescribedBy :qualityMetric;
.

:qualityMetric
	a dqv:Metric;
.""" .

    <https://alex-randles.github.io/MQV/#mappingPurpose> a ns4:DatatypeProperty ;
        rdfs:label "mappingPurpose"@en ;
        rdfs:comment "The rationale for the mapping document being created." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingDocument> ;
        rdfs:range xsd:string ;
        rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingDocument
	a mqv:MappingDocument;
	mqv:mappingPurpose 'A mapping for mapping personnel information to RDF.^^xsd:string'""" .

    <https://alex-randles.github.io/MQV/#mappingSource> a ns4:ObjectProperty ;
        rdfs:label "mappingSource"@en ;
        rdfs:comment "The original mapping document, which the refined mapping document was created from." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingDocument> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingDocument> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:refinedMappingDocument
	a mqv:MappingDocument;
	mqv:mappingSource :originalMappingDocument;
.

:originalMappingDocument
	a mqv:MappingDocument;
.""" .

    <https://alex-randles.github.io/MQV/#performedBy> a ns4:ObjectProperty ;
        rdfs:label "performedBy"@en ;
        rdfs:comment "The agent who executes the mapping quality assessment process." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <http://www.w3.org/ns/prov#Agent> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:performedBy :mappingEngineer;
.

:mappingEngineer
	a prov:Agent;
.""" .

    <https://alex-randles.github.io/MQV/#performedOn> a ns4:ObjectProperty ;
        rdfs:label "performedOn"@en ;
        rdfs:comment "The original mapping document which the quality assessment process was performed on." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingDocument> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:performedOn :mappingDocument;
.

:mappingDocument
	a mqv:MappingDocument;
.""" .

    <https://alex-randles.github.io/MQV/#refinedViolation> a ns4:ObjectProperty ;
        rdfs:label "refinedViolation"@en ;
        rdfs:comment "The refinement which refined the violation." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingRefinement> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingViolation> ;
        rdfs:subPropertyOf <http://www.w3.org/ns/prov#generated> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:refinement-0
	a mqv:MappingRefinement;
	mqv:refinedViolation :mappingViolation.

:mappingViolation
    a mqv:MappingViolation .

""" .

    <https://alex-randles.github.io/MQV/#refinementName> a ns4:DatatypeProperty ;
        rdfs:label "refinementName"@en ;
        rdfs:comment "A textual description of a refinement." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingRefinement> ;
        rdfs:range xsd:string ;
        rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:refinement-0
	a mqv:MappingRefinement;
	mqv:refinementName "Remove Language Tag"^^xsd:string.
""" .

    <https://alex-randles.github.io/MQV/#refinementPerformedBy> a ns4:ObjectProperty ;
        rdfs:label "refinementPerformedBy"@en ;
        rdfs:comment "The agent who performed the refinement of the mapping." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        rdfs:range <http://www.w3.org/ns/prov#Agent> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingAssessment
	a mqv:MappingAssessment;
	mqv:refinementPerformedBy :AlexRandles;
.

:AlexRandles
	a prov:Agent;
.""" .

    <https://alex-randles.github.io/MQV/#resultMessage> a ns4:DatatypeProperty ;
        rdfs:label "resultMessage"@en ;
        rdfs:comment "A textual description of the violation." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingViolation> ;
        rdfs:range xsd:string ;
        rdfs:subPropertyOf <http://www.w3.org/ns/prov#value> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingViolation
    a mqv:MappingViolation;
    mqv:resultMessage "Usage of undefined Property."
.

""" .

    <https://alex-randles.github.io/MQV/#toolsUsed> a ns4:ObjectProperty ;
        rdfs:label "toolsUsed"@en ;
        rdfs:comment "Mapping tools which have been used to create a mapping document." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingDocument> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingTool> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingDocument
	a mqv:MappingDocument;
	mqv:toolsUsed :mappingTool;
.

:mappingTool
	a mqv:MappingTool;
.""" .

    <https://alex-randles.github.io/MQV/#wasDerivedFrom> a ns4:ObjectProperty ;
        rdfs:label "wasDerivedFrom" ;
        rdfs:comment "The quality measurement calculated by the mapping quality assessment process." ;
        rdfs:domain <http://www.w3.org/ns/dqv#QualityMeasurement> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingAssessment> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:qualityMeasurement
	a dqv:QualityMeasurement;
	mqv:wasDerivedFrom :mappingAssessment;
.

:mappingAssessment
	a mqv:MappingAssessment;
.""" .

    <https://alex-randles.github.io/MQV/#wasRefinedBy> a ns4:ObjectProperty ;
        rdfs:label "wasRefinedBy"@en ;
        rdfs:comment "The refinement used to refine a quality violation." ;
        rdfs:domain <https://alex-randles.github.io/MQV/#MappingViolation> ;
        rdfs:range <https://alex-randles.github.io/MQV/#MappingRefinement> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingViolation
	a mqv:MappingViolation;
	mqv:wasRefinedBy :mappingRefinement;
.

:mappingRefinement
	a mqv:MappingRefinement;
.""" .

    <http://www.w3.org/ns/dqv#Category> a ns4:Class .

    <http://www.w3.org/ns/dqv#Dimension> a ns4:Class .

    <http://www.w3.org/ns/prov#Collection> a ns4:Class .

    <http://www.w3.org/ns/prov#generated> a ns4:ObjectProperty .

    <https://alex-randles.github.io/MQV/#MappingTool> a ns4:Class ;
        rdfs:label "MappingTool"@en ;
        rdfs:comment "A tool which is used to create or refine a mapping document." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:mappingTool
	a mqv:MappingTool;
.

:mappingDocument
	a mqv:MappingDocument ;
	mqv:toolsUsed :mappingTool ;
.""" .

    <https://alex-randles.github.io/MQV/#QualityMetricType> a ns4:Class ;
        rdfs:label "QualityMetricType"@en ;
        rdfs:comment "Quality metrics may be grouped into type's, such as mapping, data and vocabulary." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Collection> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:dataQualityType
	a mqv:QualityMetricType;
.

:mappingViolation
	a mqv:MappingViolation ;
	mqv:hasMetricType :dataQualityType ;
.
""" .

    <http://www.w3.org/ns/dqv#QualityMeasurement> a ns4:Class .

    <http://www.w3.org/ns/prov#Activity> a ns4:Class .

    <https://alex-randles.github.io/MQV/#MappingValidationReport> a ns4:Class ;
        rdfs:label "MappingValidationReport"@en ;
        rdfs:comment "A mapping validation report contains information about quality violations contained within a mapping document." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:mappingValidationReport
	a mqv:MappingValidationReport;
.

:mappingAssessment
	a mqv:MappingAssessment ;
	mqv:hasValidationReport :mappingValidationReport ;
.
""" .

    <https://alex-randles.github.io/MQV/#QualityRequirement> a ns4:Class ;
        rdfs:label "QualityRequirement"@en ;
        rdfs:comment "A quality requirement is a requirement a mapping should satisfy." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:qualityRequirement
	a mqv:QualityRequirement;
.

:mappingAssessment
	a mqv:MappingAssessment ;
	mqv:hasQualityRequirement :qualityRequirement ;
.""" .

    <http://www.w3.org/ns/prov#Agent> a ns4:Class .

    <http://www.w3.org/ns/prov#value> a ns4:DatatypeProperty .

    <http://www.w3.org/ns/dqv#Metric> a ns4:Class .

    <http://www.w3.org/ns/prov#Entity> a ns4:Class .

    <https://alex-randles.github.io/MQV/#MappingRefinement> a ns4:Class ;
        rdfs:label "MappingRefinement"@en ;
        rdfs:comment "An activity which involves removing quality violations contained within a mapping document." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Activity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:mappingRefinement
	a mqv:MapppingRefinement;
.

:mappingViolation
	a mqv:Violation ;
	mqv:wasRefinedBy :mappingRefinement ;
.
""" .

    <https://alex-randles.github.io/MQV/#MappingDocument> a ns4:Class ;
        rdfs:label "MappingDocument"@en ;
        rdfs:comment "A mapping document is a document which contains information on how non-RDF data will be transformed to RDF." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:mappingDocument
	a mqv:MappingDocument;
	mqv:createdBy :mappingEngineer
.

:mappingEngineer
	a prov:Agent ;
.
""" .

    <https://alex-randles.github.io/MQV/#MappingViolation> a ns4:Class ;
        rdfs:label "MappingViolation"@en ;
        rdfs:comment "A mapping violation is a quality issue present within a mapping." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;


:mappingViolation
	a mqv:MappingViolation;
.

:mappingValidationReport
	a mqv:MappingValidationReport ;
	mqv:hasViolation :mappingViolation;
.""" .

    <https://alex-randles.github.io/MQV/#MappingAssessment> a ns4:Class ;
        rdfs:label "MappingAssessment"@en ;
        rdfs:comment "An activity in which the quality of a mapping document is assessed, generating information on quality issues within the mapping." ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Activity> ;
        ns2:example """@prefix :     <http://example.com/>.;
@prefix mqv: <https://alex-randles.github.io/MQV/#>.;

:mappingQualityAssessement
	a mqv:MappingAssessment ;
	mqv:perfomedBy :mappingEngineer;
.

:mappingEngineer
    a  prov:Agent;
""" .
}


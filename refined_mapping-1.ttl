 	 rr:predicate  prov:generatedAtTime ;	
	 rr:objectMap  [	
		 rr:termType  rr:IRI ;	
		 rr:datatype  xsd:dateTime ;	
		 rr:column  "ENAME" ;	
		]; 	
 ];	
.	
	

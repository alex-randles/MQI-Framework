@prefix xml: <http://www.w3.org/XML/1998/namespace> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix rr: <http://www.w3.org/ns/r2rml#> . 
@prefix ex: <http://example.com/ns#> . 
@prefix foaf: <http://xmlns.com/foaf/0.1/> . 
@prefix prov: <http://www.w3.org/ns/prov#> . 
@prefix dqv: <http://www.w3.org/ns/dqv#> . 
@prefix geo: <http://www.opengis.net/ont/geosparql#> . 
@prefix cidoc: <http://erlangen-crm.org/current/> . 
@prefix mqio: <https://www.w3.org/MQIO/#> . 
@prefix fvc: <http://www.w3id.org/FAIRVASC#> . 

	
<#TriplesMap1>	
	
 rr:logicalTable [ 	
 	 rr:tableName  "EMP" ;	
 ];	
	
 rr:subjectMap [ 	
 	 rr:class  prov:Test , foaf:Employee ;	
	 rr:template  "http://data.example.com/employee/{EMPNO}" ;	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate  prov:generatedAtTime ;	
	 rr:objectMap  [	
		 rr:termType  rr:IRI ;	
		 rr:column  "ENAME" ;	
		 rr:datatype  xsd:dateTime ;	
		]; 	
 ];	
.	
	
<#TriplesMap2>	
	
 rr:logicalTable [ 	
 	 rr:tableName  "EMP" ;	
 ];	
	
 rr:subjectMap [ 	
 	 rr:template  "http://data.example.com/employee/{EMPNO}" ;	
	 rr:class  foaf:OnlineChatAccount , foaf:Employee ;	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate  foaf:name ;	
	 rr:objectMap  [	
		 rr:language  "en-us" ;	
		 rr:column  "ENAME" ;	
		 rr:datatype  xsd:positiveInteger ;	
		]; 	
 ];	
.	

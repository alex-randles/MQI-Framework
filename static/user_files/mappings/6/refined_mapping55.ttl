@prefix xml: <http://www.w3.org/XML/1998/namespace> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix ibclo: <https://ibclo.ericsson.com/#> . 
@prefix rml: <http://semweb.mmlab.be/ns/rml#> . 
@prefix rr: <http://www.w3.org/ns/r2rml#> . 

	
<#Metric>	
	
 rml:logicalSource [ 	
 	 rml:source "metric-data-v1.csv";	
 ];	
	
 rr:subjectMap [ 	
 	 rr:template "http://data.example.com/metric/{METRIC_ID}";	
	 rr:class ibclo:Metric;	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate ibclo:hasTimestamp;	
	 rr:objectMap [	
		 rr:column "__NAME__";	
		]; 	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate ibclo:hasDevice;	
	 rr:objectMap [	
		 rr:column "path";	
		]; 	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate ibclo:hasResultSet;	
	 rr:objectMap [	
		 rr:parentTriplesMap <#MetricResults> ;	
		 rr:joinCondition [	
			 rr:parent "lkkkk", "RESULT_ID";	
			 ];	
 		]; 	
 ];	
.	

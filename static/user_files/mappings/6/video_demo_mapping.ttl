@prefix ibclo: <https://ibclo.ericsson.com/#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .

<file:///home/alex/MQI-Framework/static/uploads/mappings/video_demo_mapping.ttl#Metric> rml:logicalSource [ rml:source "metric-data-v1.csv" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:column "__NAME__" ] ;
            rr:predicate ibclo:hasNames ],
        [ rr:objectMap [ rr:column "path" ] ;
            rr:predicate ibclo:hasDevice ],
        [ rr:objectMap [ rr:joinCondition [ rr:parent "RESULT_ID" ] ;
                    rr:parentTriplesMap <file:///home/alex/MQI-Framework/static/uploads/mappings/video_demo_mapping.ttl#MetricResults> ] ;
            rr:predicate ibclo:hasResultSet ] ;
    rr:subjectMap [ rr:class ibclo:Metric ;
            rr:template "http://data.example.com/metric/{METRIC_ID}" ] .


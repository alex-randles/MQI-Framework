@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#>.

<#TriplesMap8>
    rr:logicalTable [
                     rr:tableName "metric-data-v1.csv";
                     ];
    rr:subjectMap [
        rr:template "http://data.example.com/person/{testing}";
        rr:class foaf:Agent;

    ];
    rr:predicateObjectMap [
        rr:predicate foaf:knows ;
        rr:object <http://data.example.com/friend/1>
    ];
    rr:predicateObjectMap [
        rr:predicate foaf:names ;
        rr:objectMap [
                         rr:column "NAME";
                     ];
    ];
    rr:predicateObjectMap [
        rr:predicate foaf:based_near ;
        rr:objectMap [
                         rr:template "http://data.example.com/location/{person_id}";
                         rr:termType rr:Literal;
                     ];
    ];
.

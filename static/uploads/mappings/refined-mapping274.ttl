@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix rr: <http://www.w3.org/ns/r2rml#> . 
@prefix ont: <http://ontologies.geohive.ie/osi#> . 
@prefix geo: <http://www.opengis.net/ont/geosparql#> . 
@prefix activity: <http://activitystrea.ms/specs/atom/1.0/> . 

	
<#TriplesMap1>	
	
 rr:logicalTable [ 	
 	 rr:tableName "student.csv" ;	
 ];	
	
 rr:subjectMap [ 	
 	 rr:class activity:class-value-1;	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate rdfs:label;	
	 rr:objectMap [	
		 rr:language "en" ;	
		 rr:column "NAME" ;	
		]; 	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate rdfs:label;	
	 rr:objectMap [	
		 rr:column "ENGLISH" ;	
		]; 	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate geo:hasGeometry;	
	 rr:objectMap [	
		 rr:template "urn:osi:boundary:geom:{GUID}" ;	
		]; 	
 ];	
.	
	
<#TriplesMap2>	
	
 rr:logicalTable [ 	
 	 rr:tableName "LEA_CBD2019_20M" ;	
 ];	
	
 rr:subjectMap [ 	
 	 rr:template "urn:osi:boundary:geom:{GUID}" ;	
	 rr:class geo:Geometry;	
 ];	
	

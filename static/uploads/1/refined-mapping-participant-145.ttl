<#TriplesMap1>	
	rml:logicalSource [
  rml:source "TRANSPORT.BUS" ;
  rml:referenceFormulation rr:SQL2008;
]; 
	
 rr:subjectMap [ 	
 	 rr:template  "http://data.example.com/employee/{EMPNO}" ;	
	 rr:class  prov:Entity , foaf:Document ;	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate  prov:generatedAtTime ;	
	 rr:objectMap  [	
		 rr:column  "ENAME" ;	
		]; 	
 ];	

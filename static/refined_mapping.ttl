@prefix xml: <http://www.w3.org/XML/1998/namespace> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix rr: <http://www.w3.org/ns/r2rml#> . 
@prefix foaf: <http://xmlns.com/foaf/0.1/> . 

	
<#TriplesMap8>	
	
 rr:logicalTable [ 	
 	 rr:tableName "PEOPLE";	
 ];	
	
 rr:subjectMap [ 	
 	 rr:class foaf:OnlineChatAccount, foaf:Agent;	
	 rr:template "http://data.example.com/person/{person_id}";	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate foaf:fundedBy;	
	 rr:objectMap [	
		 rr:column "NAME";	
		]; 	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate foaf:gender;	
	 rr:objectMap [	
		 rr:column "GENDER";	
		]; 	
 ];	
	
 rr:predicateObjectMap [ 	
 	 rr:predicate foaf:based_near;	
	 rr:objectMap [	
		 rr:template "http://data.example.com/location/{person_id}";	
		 rr:termType rr:Literal;	
		]; 	
 ];	
.	

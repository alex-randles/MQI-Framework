@prefix ex: <http://example.org/> .
@prefix mqv: <https://alex-randles.github.io/MQV#> .
@prefix mqv-metric: <https://alex-randles.github.io/MQV-METRICS#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:mappingQualityAssessment a mqv:MappingAssessment ;
    mqv:assessedMapping <mapping.ttl> ; 
    mqv:executes mqv-metric:D1,
				mqv-metric:D2,
				mqv-metric:D3,
				mqv-metric:D4,
				mqv-metric:D5,
				mqv-metric:D6,
				mqv-metric:D7,
				mqv-metric:MP1,
				mqv-metric:MP2,
				mqv-metric:MP3,
				mqv-metric:MP4,
				mqv-metric:MP5,
				mqv-metric:MP6,
				mqv-metric:MP7,
				mqv-metric:MP8,
				mqv-metric:VOC1,
				mqv-metric:VOC2,
				mqv-metric:VOC3,
				mqv-metric:VOC4,
				mqv-metric:VOC5;
	mqv:hasValidationReport ex:mappingValidationReport .

ex:mappingValidationReport a mqv:MappingValidationReport ;
    rdfs:comment "Validation report containing quality violations (if any) ." .



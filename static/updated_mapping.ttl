@prefix openfit: <http://ontology.openfit.org#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .

<file:///home/alex/MQI-Framework/static/uploads/mappings/openfit-mapping_copy.ttl#Exercise> a rr:TriplesMap ;
    rr:logicalTable [ rr:sqlQuery "SELECT Goal.id, Goal.description, Goal.type, Goal.type_id FROM loans-v1.csv" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:column "Married" ] ;
            rr:predicate openfit:descriptions ] ;
    rr:subjectMap [ rr:class openfit:CardioExercise,
                openfit:StrengthExercise ;
            rr:template "http://data.ontology.openfit.org#goal_{id}" ] .

<file:///home/alex/MQI-Framework/static/uploads/mappings/openfit-mapping_copy.ttl#MuscleGain> a rr:TriplesMap ;
    rr:logicalTable [ rr:sqlQuery "SELECT Goal.id,Goal.description,Goal.type,Goal.type_id, MuscleGainTargetMuscles.targetMuscle_id FROM (MuscleGain JOIN MuscleGainTargetMuscles ON (MuscleGain.id = MuscleGainTargetMuscles.muscleGain_id))JOIN Goal ON (Goal.type_id = MuscleGain.id) WHERE Goal.type = 'muscleGain'" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:joinCondition [ rr:child "targetMuscle_id" ;
                            rr:parent "id" ] ;
                    rr:parentTriplesMap <file:///home/alex/MQI-Framework/static/uploads/mappings/openfit-mapping_copy.ttl#TargetMuscle> ] ;
            rr:predicate openfit:conditioning_id ] ;
    rr:subjectMap [ rr:class openfit:MuscleGain ;
            rr:template "http://data.ontology.openfit.org#goal_{id}" ] .

<file:///home/alex/MQI-Framework/static/uploads/mappings/openfit-mapping_copy.ttl#TargetArea> a rr:TriplesMap ;
    rr:logicalTable [ rr:sqlQuery "SELECT  * FROM TargetArea" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:column "Dependents" ] ;
            rr:predicate openfit:target_area_id ],
        [ rr:objectMap [ rr:column "name" ] ;
            rr:predicate openfit:name ] ;
    rr:subjectMap [ rr:class openfit:Target_Area ;
            rr:template "http://data.ontology.openfit.org#target_area_{id}" ] .


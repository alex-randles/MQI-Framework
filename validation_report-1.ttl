ex:refinement-0 a mqio:MappingRefinement ;
    prov:endedAtTime "2023-04-08T19:52:46.768520"^^xsd:dateTime ;
    mqio:hasRefinementQuery """
                PREFIX rr: <http://www.w3.org/ns/r2rml#> 
                DELETE {   ?pom rr:predicate <http://xmlns.com/foaf/0.1/lastName>  .  }
                INSERT {   ?pom rr:predicate <http://www.snik.eu/ontology/bb/kmdoiook> .     }
                WHERE {
                    ?pom rr:predicate <http://xmlns.com/foaf/0.1/lastName>  .
                    FILTER(str(?pom) = "ub42bL19C27").
                }
                """^^xsd:string ;
    mqio:refinedViolation ex:violation-0 ;
    mqio:refinementName "Change Predicate"^^xsd:string .

ex:refinement-1 a mqio:MappingRefinement ;
    prov:endedAtTime "2023-04-08T19:52:46.799361"^^xsd:dateTime ;
    mqio:hasRefinementQuery """
                    PREFIX rr: <http://www.w3.org/ns/r2rml#> 
                    INSERT { ?joinCondition rr:child "dddd" } 
                    WHERE { 
                    SELECT ?joinCondition
                    WHERE {
                          ?subject rr:joinCondition ?joinCondition.
                          FILTER(str(?joinCondition) = "ub42bL15C26").
                        }
                    }
                   """^^xsd:string ;
    mqio:refinedViolation ex:violation-1 ;
    mqio:refinementName "Add Child Column"^^xsd:string .

